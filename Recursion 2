Tower of Hanoi:

public class TowerOfHanoi {
  
	public static void towerOgHanoi(int n,String src,String helper,String dest)
	{
		if(n==1)
		{
			System.out.println("Transfer disc"+n+" from "+src+" to"+dest);
			return;
		}
		towerOgHanoi(n-1, src,dest,helper);
		System.out.println("Transfer disc"+n+" from "+src+" to"+dest);
		towerOgHanoi(n-1,helper,src,dest);
	}

	public static void main(String[] args) {
		int n=3;
		towerOgHanoi(n,"S","H","D");

	}

}
